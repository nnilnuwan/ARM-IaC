
{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "environmentName": { "value": "cae-dev-no-vnet" },
    "location": { "value": "eastus" },
    "useExistingVNet": { "value": "no" },

    // ZR must remain Disabled when useExistingVNet = "no"
    "zoneRedundancy": { "value": "Disabled" },

    "publicNetworkAccess": { "value": "Enabled" },

    // Monitoring: don't save logs
    "logsDestination": { "value": "none" },

    // No workload profiles -> consumption-only environment
    "workloadProfiles": { "value": [] }
   }
